{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@nestjs-ext/cloud-config/(.*)": "<rootDir>/../libs/cloud-config/src/$1",
    "@nestjs-ext/cloud-config": "<rootDir>/../libs/cloud-config/src"
  }
}
