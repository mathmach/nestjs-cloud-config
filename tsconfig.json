{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "es2017",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "paths": {
      "@nestjs-ext/cloud-config": [
        "libs/cloud-config/src"
      ],
      "@nestjs-ext/cloud-config/*": [
        "libs/cloud-config/src/*"
      ]
    }
  }
}