{
  "name": "nestjs-cloud-config",
  "version": "1.0.12",
  "description": "",
  "author": "Matheus Machado Guerzoni Duarte <mmgd23@gmail.com>",
  "private": false,
  "license": "UNLICENSED",
  "main": "dist/libs/cloud-config/index.js",
  "types": "dist/libs/cloud-config/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mathmach/nestjs-cloud-config.git"
  },
  "bugs": {
    "url": "https://github.com/mathmach/nestjs-cloud-config/issues"
  },
  "homepage": "https://github.com/mathmach/nestjs-cloud-config#readme",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build cloud-config --tsc",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "deploy": "npm run build && npm publish"
  },
  "dependencies": {
    "@nestjs/common": "^8.4.3",
    "@nestjs/config": "^2.0.0",
    "@nestjs/core": "^8.4.3",
    "@nestjs/platform-express": "^8.4.3",
    "extend": "^3.0.2",
    "joi": "^17.4.0",
    "js-yaml": "^4.0.0",
    "lodash": "^4.17.20",
    "nice-cloud-config-client": "^1.0.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.5"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.2.4",
    "@nestjs/schematics": "^8.0.8",
    "@nestjs/testing": "^8.4.3",
    "@types/chai": "^4.2.14",
    "@types/chai-as-promised": "^7.1.3",
    "@types/express": "^4.17.8",
    "@types/extend": "^3.0.1",
    "@types/jest": "^27.4.1",
    "@types/joi": "^17.2.3",
    "@types/js-yaml": "^4.0.0",
    "@types/lodash": "^4.14.168",
    "@types/node": "^17.0.23",
    "@types/sinon": "^10.0.11",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "chai": "^4.3.0",
    "chai-as-promised": "^7.1.1",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.5.1",
    "prettier": "^2.1.2",
    "sinon": "^13.0.1",
    "supertest": "^6.0.0",
    "ts-jest": "^27.1.4",
    "ts-loader": "^9.2.8",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "@nestjs-ext/cloud-config/(.*)": "<rootDir>/libs/cloud-config/src/$1",
      "@nestjs-ext/cloud-config": "<rootDir>/libs/cloud-config/src"
    }
  }
}
